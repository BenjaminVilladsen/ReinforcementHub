	<template>
		<name>MountainCar</name>
		<declaration>///Number of wins
int num_of_wins;

///Number of deaths
int num_of_deaths;

///GForce applied to car
double force = 0;


///Constants
const double force_mag = 0.001;
const double gravity = 0.0025;


///Thresholds
const double car_pos_min = -1.2;
const double car_pos_max = 0.6;
const double car_vel_min = -0.07;
const double car_vel_max = 0.07;

///Goal pos
const double goal_pos = 0.5;


/// Position of car on the x-axis and velocity
clock car_pos, car_vel;

double velocity() {
    return car_vel + force - cos(3 * car_pos) * gravity;
}

double position() {
    return car_pos + velocity();
}</declaration>
